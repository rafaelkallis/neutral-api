  "scripts": {
    "build": "tsc",
    "start:prod": "node -r tsconfig-paths/register build/main.js",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev:watch": "ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register src/main.ts",
    "type": "tsc --noEmit",
    "type:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts src",
    "test": "jest --verbose --config jest.config.json",
    "test:watch": "jest --config jest.config.json --watch",
    "test:cov": "jest --config jest.config.json --coverage",
    "test:e2e": "jest --verbose --config jest.e2e-config.json",
    "test:e2e:watch": "jest --verbose --config jest.e2e-config.json --watch",
    "test:e2e:cov": "jest --config jest.e2e-config.json --coverage"
  },
