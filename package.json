  "scripts": {
    "start": "node build/main.js",
    "build": "ttsc",
    "dev": "NODE_ENV=development TS_NODE_COMPILER=ttypescript ts-node-dev --no-notify --respawn src/main.ts",
    "dev:debug": "NODE_ENV=development TS_NODE_COMPILER=ttypescript ts-node-dev --inspect-brk --no-notify --respawn src/main.ts",
    "type": "ttsc --noEmit",
    "type:watch": "ttsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts src",
    "test": "jest --verbose --config jest.config.json",
    "test:watch": "jest --config jest.config.json --watch",
    "test:cov": "jest --config jest.config.json --coverage",
    "test:ci": "jest --ci --config jest.config.json --coverage",
    "test:e2e": "jest --verbose --config jest.e2e-config.json",
    "test:e2e:watch": "jest --verbose --config jest.e2e-config.json --watch",
    "test:e2e:cov": "jest --config jest.e2e-config.json --coverage",
    "test:e2e:ci": "jest --ci --config jest.e2e-config.json --coverage"
  },
